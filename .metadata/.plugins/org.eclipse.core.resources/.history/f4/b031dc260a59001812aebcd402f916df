package statistics;

public class BasicValue {

	public static int getMeanValue(Float... args) {

		Float result = 0;
		if (args != null) {
			int length = args.length;
			Float sum = 0;
			for (int i = 0; i < length; i++) {
				sum+=args[i];
			}
			
			result = sum / length;
			
		}
		return 0;
	}
}
